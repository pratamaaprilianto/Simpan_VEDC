                android:layout_width="48dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="3dp"
                android:fontFamily="@font/seguisb"
                android:text="07.43.45"
                android:textColor="@color/white"
                android:textSize="11dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView99"
                app:layout_constraintEnd_toEndOf="@+id/imageView99"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/totalat"
                app:layout_constraintTop_toTopOf="@+id/imageView99" />

            <TextView
                android:id="@+id/pulang"
                android:layout_width="48dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="10dp"
                android:fontFamily="@font/seguisb"
                android:text="07.43.45"
                android:textColor="@color/white"
                android:textSize="12dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView969"
                app:layout_constraintEnd_toEndOf="@+id/imageView969"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/plg"
                app:layout_constraintTop_toTopOf="@+id/imageView969" />

            <TextView
                android:id="@+id/ptgn"
                android:layout_width="48dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:fontFamily="@font/seguisb"
                android:text="Potongan"
                android:textColor="@color/white"
                android:textSize="10dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView9"
                app:layout_constraintStart_toStartOf="@+id/imageView9"
                app:layout_constraintTop_toTopOf="@+id/imageView9" />

            <TextView
                android:id="@+id/potongan"
                android:layout_width="42dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:fontFamily="@font/seguisb"
                android:text="-"
                android:textColor="@color/white"
                android:textSize="11dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView9"
                app:layout_constraintEnd_toEndOf="@+id/imageView9"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/ptgn"
                app:layout_constraintTop_toTopOf="@+id/imageView9"
                app:layout_constraintVertical_bias="0.625" />

            <TextView
                android:id="@+id/status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="8dp"
                android:fontFamily="@font/seguisb"
                android:text="Tidak ganti jam"
                android:textColor="@color/white"
                android:textSize="11dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView99d"
                app:layout_constraintEnd_toEndOf="@+id/imageView99d"
                app:layout_constraintStart_toEndOf="@+id/stat"
                app:layout_constraintTop_toTopOf="@+id/imageView99d" />

            <TextView
                android:id="@+id/stat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="28dp"
                android:fontFamily="@font/seguisb"
                android:text="Status :"
                android:textColor="@color/white"
                android:textSize="11dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView99d"
                app:layout_constraintStart_toStartOf="@+id/imageView99d"
                app:layout_constraintTop_toTopOf="@+id/imageView99d" />

